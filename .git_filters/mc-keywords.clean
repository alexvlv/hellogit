#!/usr/bin/perl -p
#
# @brief  Git filter to implement rcs keyword expansion as seen in cvs and svn.
# @author Martin Turon & LRO
#
# Copyright (c) 2009-2011 Turon Technologies, Inc.  All rights reserved.

s/\$All[^\$]*\$/\$All\$/; 
s/\$Date[^\$]*\$/\$Date\$/;
s/\$Author[^\$]*\$/\$Author\$/; 
s/\$Source[^\$]*\$/\$Source\$/; 
s/\$File[^\$]*\$/\$File\$/; 
s/\$Revision[^\$]*\$/\$Revision\$/;
s/\$Version[^\$]*\$/\$Version\$/;
